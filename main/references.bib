	author = {Cutts, Diana Becker and Meyers, Alan F. and Black, Maureen M. and Casey, Patrick H. and Chilton, Mariana and Cook, John T. and Geppert, Joni and Ettinger De Cuba, Stephanie and Heeren, Timothy and Coleman, Sharon and Rose-Jacobs, Ruth and Frank, Deborah A.},
	month = aug,
	year = {2011},
	pages = {1508--1514},
	file = {Cutts et al. - 2011 - US Housing Insecurity and the Health of Very Young.pdf:/home/steve-garcia/Zotero/storage/CNLVX8AR/Cutts et al. - 2011 - US Housing Insecurity and the Health of Very Young.pdf:application/pdf},
}

@article{durst_contribution_2019,
