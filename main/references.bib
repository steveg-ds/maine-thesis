	pages = {405--424},
	file = {Preis et al. - 2021 - Mapping gentrification and displacement pressure .pdf:/home/steve-garcia/Zotero/storage/PKM9VQGB/Preis et al. - 2021 - Mapping gentrification and displacement pressure .pdf:application/pdf},
}

