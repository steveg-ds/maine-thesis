	file = {Deitz and Meehan - 2019 - Plumbing Poverty Mapping Hot Spots of Racial and .pdf:/home/steve-garcia/Zotero/storage/I5MGA7UJ/Deitz and Meehan - 2019 - Plumbing Poverty Mapping Hot Spots of Racial and .pdf:application/pdf},
