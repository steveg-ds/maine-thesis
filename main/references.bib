	file = {Sherrieb et al. - 2010 - Measuring Capacities for Community Resilience.pdf:/home/steve-garcia/Zotero/storage/6MJFSRU8/Sherrieb et al. - 2010 - Measuring Capacities for Community Resilience.pdf:application/pdf},
