@article{goetz_us_2010,
	title = {U.{S}. {Commuting} {Networks} and {Economic} {Growth}: {Measurement} and {Implications} for {Spatial} {Policy}: {U}.{S}. {COMMUTING} {NETWORKS} {AND} {ECONOMIC} {GROWTH}},
