	year = {2020},
	note = {Publisher: Mary Ann Liebert, Inc., publishers},
	pages = {79--100},
	file = {Full Text PDF:/home/steve-garcia/Zotero/storage/DWFVJ8KS/Powell et al. - 2020 - Partisan Gerrymandering, Clustering, or Both A Ne.pdf:application/pdf},
}

